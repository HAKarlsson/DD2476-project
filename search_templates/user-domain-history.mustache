{
  "template": {
    "size": 100,
    "sort": [
      {
        "day": "desc"
      },
      {
        "serpId": "asc"
      }
    ],
    "query": {
      "bool": {
        "must": [
          {
            "match": {
              "user": "{{user}}"
            }
          },
          {
            "nested": {
              "path": "documents",
              "query": {
                "match": {
                  "documents.domain": "{{domain}}"
                }
              }
            }
          },
          {
            "range": {
              "day": {
                "lt": "{{day}}"
              }
            }
          }
        ]
      }
    },
    "aggs": {
      "domain": {
        "nested": {
          "path": "documents"
        },
        "aggs": {
          "relevance": {
            "extended_stats": {
              "field": "documents.relevance"
            }
          },
          "position": {
            "extended_stats": {
              "field": "documents.position"
            }
          },
          "clicks": {
            "extended_stats": {
              "field": "documents.clicks"
            }
          }
        }
      }
    }
  }
}