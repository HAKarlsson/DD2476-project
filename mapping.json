DELETE yandex

PUT yandex
{
  "settings": {
      "number_of_shards" :   3,
      "number_of_replicas" : 0,
      "refresh_interval" : "2s"
  },
  "mappings": {
    "session": {
      "_all": {
        "enabled": false
      },
      "properties": {
        "user": {
          "type": "integer"
        },
        "day": {
          "type": "integer"
        }
      }
    },
    "serp": {
      "_all": {
        "enabled": false
      },
      "_parent": {
        "type": "session"
      },
      "properties": {
        "serpId": {
          "type": "integer"
        },
        "timePassed": {
          "type": "integer"
        },
        "query": {
          "type": "text"
        },
        "isTest": {
          "type": "boolean"
        },
        "serpitems": {
          "type": "nested"
        }
      }
    }
  }
}